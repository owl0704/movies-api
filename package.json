{
  "name": "movies-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all clean compile:all",
    "clean": "rimraf ./build && mkdir build",
    "compile:moviemodels": "babel ./api/movies --out-dir build/api/movies",
    "compile:usermodels": "babel ./api/users --out-dir build/api/users",
    "compile:models": "npm-run-all compile:moviemodels compile:usermodels",
    "compile:seed": "babel ./seedData --out-dir build/seedData",
    "compile:authenticate": "babel ./authenticate --out-dir build/authenticate",
    "compile:routes": "babel ./routes --out-dir build/routes ",
    "compile:app": "babel index.js --out-dir build/",
    "compile:db": "babel db.js --out-dir build/",
    "compile:all": "npm-run-all compile:models compile:seed compile:authenticate compile:app compile:db ",
    "postinstall": "rimraf ./build && npm run compile:all",
    "start": "nodemon --exec babel-node index.js",
    "test": "cross-env NODE_ENV=test mocha --timeout 10000"
  },
  "author": "zhixiang li",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.3.4",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "cross-env": "^6.0.3",
    "eslint": "^7.17.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "bcrypt": "^5.0.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.1",
    "mongoose": "^5.11.11",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "rimraf": "^3.0.0"
  }
}
